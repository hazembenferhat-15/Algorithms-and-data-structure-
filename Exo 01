#include <stdio.h>

int main() {
    int n,i,j;
    int A[50][50]; 

    printf("Enter size of the matrix n: ");
    scanf("%d", &n);

    printf("Enter the elements of the matrix:\n");
    for ( i = 0; i < n; i++) {
        for ( j = 0; j < n; j++) {
            scanf("%d", &A[i][j]);
        }
    }
      int x;
    for ( i = 0; i < n; i++) {
        for ( j = i + 1; j < n; j++) {
            x = A[i][j];
            A[i][j] = A[j][i];
            A[j][i] = x;
        }
    }
    for ( i = 0; i < n; i++) {
        for ( j = 0; j < n; j++) {
            printf("%d\t", A[i][j]);
        }
        printf("\n");
    }

    return 0;
}
